              <arg:description>In the worst case (which is decisive), CE aggravates catastrophic climate impacts.</arg:description>
              <arg:proposition attacks="0" id="63147" index="1" supports="1" type="premise">
                <arg:supportReference refersToArgunetNode="63147" refersToNode="n13" refersToProposition="63147"/>
